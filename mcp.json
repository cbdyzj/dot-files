{
  "mcpServers": {
    "context7": {
      "command": "pnpx",
      "args": [
        "@upstash/context7-mcp"
      ]
    },
    "pubmed-mcp": {
      "command": "pnpx",
      "args": [
        "@cyanheads/pubmed-mcp-server"
      ],
      "env": {
        "NCBI_API_KEY": "${NCBI_API_KEY}"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}